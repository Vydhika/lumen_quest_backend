{
  "name": "lumen_quest_backend",
  "version": "1.0.0",
  "description": "Backend API for Lumen Quest subscription management system",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "db:test": "node src/db/db-utils.js test",
    "db:migrate": "node src/db/db-utils.js migrate",
    "db:create-migration": "node src/db/db-utils.js create-migration",
    "db:stats": "node src/db/db-utils.js stats",
    "db:cleanup": "node src/db/db-utils.js cleanup",
    "db:backup": "node src/db/db-utils.js backup",
    "db:analyze": "node src/db/db-utils.js analyze",
    "db:reset": "node src/db/db-utils.js reset",
    "db:seed-dev": "node src/db/db-utils.js seed-dev",
    "migrate:run": "node src/db/migrations/migration-runner.js run",
    "migrate:create": "node src/db/migrations/migration-runner.js create"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dipeshdahiya/lumen_quest_backend.git"
  },
  "keywords": ["subscription", "management", "api", "express", "supabase"],
  "author": "Vydhika",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dipeshdahiya/lumen_quest_backend/issues"
  },
  "homepage": "https://github.com/Dipeshdahiya/lumen_quest_backend#readme",
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.9.2",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "bcryptjs": "^2.4.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "eslint": "^8.44.0"
  }
}
